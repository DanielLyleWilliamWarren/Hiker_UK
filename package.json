    "lint:quiet": "eslint \"*/**/*.{ts,tsx}\" --quiet --fix",
    "lint:scss": "stylelint \"**/*.scss\"",
    "lint:scss:fix": "stylelint \"**/*.scss\" --fix",
    "prepare": "husky",
    "e2e": "pnpm exec playwright test --trace on",
    "e2e-debug": "PWDEBUG=1 pnpm exec playwright test",
    "ci-e2e": "start-server-and-test start:ci http://localhost:4200 e2e",
    "generate-test": "pnpm exec playwright codegen http://localhost:4200"
  },
  "private": true,
